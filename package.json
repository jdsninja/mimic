{
  "name": "mimic",
  "version": "1.0.0",
  "description": "TDB",
  "main": "src/js/mimic.js",
  "dependencies": {
    "babel": "^5.8.21"
  },
  "devDependencies": {
    "babelify": "~5.0.3",
    "browser-sync": "^1.7.1",
    "browserify": "^6.3.3",
    "gulp": "~3.9.0",
    "gulp-rename": "^1.2.0",
    "lodash": "^2.4.1",
    "rimraf": "~2.2.8",
    "watchify": "^2.1.1",
    "vinyl-source-stream": "^1.0.0"
  },
  "scripts": {
    "clean": "rimraf build",
    "build:webpack": "webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "NODE_PATH='src' node server.js",
    "debug": "NODE_PATH='src' echo 'Nothing? `npm install -g iron-node`' && iron-node src/debug.js",
    "lint": "eslint src",
    "pretest": "npm run lint",
    "test": "NODE_PATH='src' babel-node src/test/index.js",
    "watch": "watch 'clear && npm run test -s' src",
    "update": "updtr"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeromeds/mimic.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeromeds/mimic/issues"
  },
  "homepage": "https://github.com/jeromeds/mimic"
}
